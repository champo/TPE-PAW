<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>Archetype Created Web Application</display-name>

	<filter>
		<filter-name>sessionFilter</filter-name>
		<filter-class>ar.edu.itba.paw.grupo1.filter.SessionFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>sessionFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<servlet>
		<servlet-name>register</servlet-name>
		<servlet-class>ar.edu.itba.paw.grupo1.controller.RegisterServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>ar.edu.itba.paw.grupo1.controller.LoginServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>ar.edu.itba.paw.grupo1.controller.LogoutServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ListProperties</servlet-name>
		<servlet-class>ar.edu.itba.paw.grupo1.controller.ListPropertiesServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ListProperties</servlet-name>
		<url-pattern>/listProperties</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>EditProperty</servlet-name>
		<servlet-class>ar.edu.itba.paw.grupo1.controller.EditPropertyServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>EditProperty</servlet-name>
		<url-pattern>/editProperty</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>AddProperty</servlet-name>
		<servlet-class>ar.edu.itba.paw.grupo1.controller.AddPropertyServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AddProperty</servlet-name>
		<url-pattern>/addProperty</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>AddPicture</servlet-name>
		<servlet-class>ar.edu.itba.paw.grupo1.controller.AddPictureServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AddPicture</servlet-name>
		<url-pattern>/addPicture</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>EditPicture</servlet-name>
		<servlet-class>ar.edu.itba.paw.grupo1.controller.EditPictureServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>EditPicture</servlet-name>
		<url-pattern>/editPicture</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>PropertyDetail</servlet-name>
		<servlet-class>ar.edu.itba.paw.grupo1.controller.PropertyDetailServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>PropertyDetail</servlet-name>
		<url-pattern>/propertyDetail</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>PublishProperty</servlet-name>
		<servlet-class>ar.edu.itba.paw.grupo1.controller.PublishPropertyServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>PublishProperty</servlet-name>
		<url-pattern>/publishProperty</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>UnpublishProperty</servlet-name>
		<servlet-class>ar.edu.itba.paw.grupo1.controller.UnpublishPropertyServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>UnpublishProperty</servlet-name>
		<url-pattern>/unpublishProperty</url-pattern>
	</servlet-mapping>
